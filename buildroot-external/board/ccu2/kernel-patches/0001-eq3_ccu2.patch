+static int pcf8563_get_mask_value(uint8_t bcd, uint8_t mask)
+{
+	if (bcd & 0x80)
+		return -1;
+
+	bcd &= ~0x80;
+	bcd &= mask;
+
+	return bcd2bin(bcd);
+}
+
+static void pcf8563_set_mask_value(uint8_t *cur, uint8_t mask, int v)
+{
+	*cur = ((*cur & ~mask) |
+		(v<0 ? 0x80 : (bin2bcd(v) & mask)));
+}
