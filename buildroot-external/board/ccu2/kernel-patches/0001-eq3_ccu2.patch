+static ssize_t eq3loop_read(struct file *filp, char *buf, size_t count, loff_t *offset)
+{
+	struct eq3loop_connection_data *conn = filp->private_data;
+	if( conn && conn->read )
+	{
+		return conn->read( conn->channel, filp, buf, count, offset );
+	}
+	return -EFAULT;
+}
