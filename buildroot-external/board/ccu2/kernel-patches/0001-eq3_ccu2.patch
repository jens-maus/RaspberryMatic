+	alrm->time.tm_mon   = -1;
+	alrm->time.tm_year  = -1;
+	alrm->time.tm_wday  = pcf8563_get_mask_value(buf[5], 0x07);
+	alrm->time.tm_yday  = -1;
+	alrm->time.tm_isdst = -1;
+
+	return 0;
+}
