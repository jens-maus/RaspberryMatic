+#define MXS_USBPHY_CTRL_ENUTMILEVEL3		(1u << 15)
+#define MXS_USBPHY_CTRL_ENUTMILEVEL2		(1u << 14)
+#define MXS_USBPHY_CTRL_WAKEUP_IRQ		(1u << 17)
