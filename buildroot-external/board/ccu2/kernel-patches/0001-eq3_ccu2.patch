+			/* Pagewrites (8 bytes) are the relatively fast */
+			rc = i2c_smbus_write_i2c_block_data(client, (off + i), 8, shadow + i);
+
+			if (rc>=0) {
+				/* Wait for the chip to store everything. */
+				msleep(32);
+				i += 8;
+			}
+		} else {
+			rc = i2c_smbus_write_i2c_block_data(client, (off + i), (count - i), shadow + i);
