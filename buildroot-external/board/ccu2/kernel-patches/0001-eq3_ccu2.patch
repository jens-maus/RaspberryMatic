+			if (rc>=0) {
+				/* Wait for the chip to store everything. */
+				msleep(10 * (count - i));
+				i += (count - i);
+			}
+		}
+	}
+	mutex_unlock(&data->update_lock);
+	return rc < 0 ? rc : count;
