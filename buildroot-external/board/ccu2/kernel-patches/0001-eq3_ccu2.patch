+#ifndef H_LINUX_SOUND_SOC_PXA_PXA_AC97_H
+#define H_LINUX_SOUND_SOC_PXA_PXA_AC97_H
+
+#include <linux/device.h>
+#include <linux/io.h>
+#include <linux/resource.h>
+#include <linux/clk.h>
+#include <linux/mutex.h>
+#include <linux/sched.h>
