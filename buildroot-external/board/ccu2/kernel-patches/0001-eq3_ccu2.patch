+
+static ssize_t gpiosysfs_set_debounce(struct device *dev,
+				      struct device_attribute *attr,
+				      char const *buf, size_t count)
+{
+	struct gpio_pxa_pin	*g = dev ? dev_get_drvdata(dev) : 0;
