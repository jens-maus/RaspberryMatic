+static ssize_t gpiosysfs_show_id(struct device *dev, struct device_attribute *attr,
+				 char *buf)
+{
+	struct gpio_pxa_pin	*g = dev ? dev_get_drvdata(dev) : 0;
+
+	if (g==0)
+		return -ENODEV;
+
+	return sprintf(buf, "%u\n", g->id);
+}
