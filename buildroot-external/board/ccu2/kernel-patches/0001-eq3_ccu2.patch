+
+#define MXS_USBCTRL_OFFSET		0
+#define MXS_USB_PHY_CTR_FUNC_OFFSET	0x8
+#define MXS_USB_PHY_CTR_FUNC2_OFFSET	0xc
+#define MXS_USBH2CTRL_OFFSET		0x14
+
