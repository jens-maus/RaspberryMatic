+// stolen from drivers/base/class.c
+static int device_add_attrs(struct device * cd,
+			    struct device_attribute attrs[])
+{
+	int i;
+	int error = 0;
+
+	for (i = 0; attr_name(attrs[i]); i++) {
+		error = device_create_file(cd, attrs+i);
+		if (error)
+			goto Err;
+	}
+Done:
+	return error;
+Err:
+	while (--i >= 0)
+		device_remove_file(cd,attrs+i);
+	goto Done;
+}
