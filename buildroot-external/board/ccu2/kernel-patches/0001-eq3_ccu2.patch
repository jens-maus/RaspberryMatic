+	if(data->write_lock != 0)
+		return -EPERM;
+	/* Refuse to write Vaio Chips */
+	if(data->nature == VAIO)
+		return -ENXIO;
+	if(off+count>EEPROM_SIZE)
+		return -ESPIPE;
