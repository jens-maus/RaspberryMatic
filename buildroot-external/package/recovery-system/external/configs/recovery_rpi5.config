BR2_aarch64=y
BR2_cortex_a76=y
BR2_ARM64_PAGE_SIZE_16K=y
BR2_ROOTFS_LIB_DIR="lib"
BR2_ROOTFS_LIB32_DIR="lib32"
BR2_ROOTFS_POST_BUILD_SCRIPT="$(BR2_EXTERNAL_RECOVERY_SYSTEM_PATH)/board/rpi5/post-build.sh"
BR2_LINUX_KERNEL_CUSTOM_TARBALL=y
BR2_LINUX_KERNEL_CUSTOM_TARBALL_LOCATION="$(BR2_EXTERNAL_LINUX_KERNEL_CUSTOM_TARBALL_LOCATION)"
BR2_LINUX_KERNEL_PATCH="$(BR2_EXTERNAL_EQ3_PATH)/board/rpi5/kernel-patches"
BR2_LINUX_KERNEL_DEFCONFIG="bcm2712"
BR2_LINUX_KERNEL_CONFIG_FRAGMENT_FILES="$(BR2_EXTERNAL_EQ3_PATH)/kernel/6.6/global.config $(BR2_EXTERNAL_EQ3_PATH)/kernel/6.6/device-support.config $(BR2_EXTERNAL_EQ3_PATH)/kernel/6.6/security-arm64.config $(BR2_EXTERNAL_EQ3_PATH)/kernel/6.6/security.config $(BR2_EXTERNAL_EQ3_PATH)/board/rpi5/kernel.config"
BR2_LINUX_KERNEL_NEEDS_HOST_LIBELF=y
BR2_PACKAGE_I2C_TOOLS=y
BR2_PACKAGE_RPI_USERLAND=y
BR2_TARGET_ROOTFS_CPIO_UIMAGE=y
BR2_PACKAGE_MULTILIB32=y
BR2_PACKAGE_MULTILIB32_CONFIG_FRAGMENT_FILE="multilib32_arm_a76-16k.config"
BR2_PACKAGE_WIRINGPI_RPI=y
