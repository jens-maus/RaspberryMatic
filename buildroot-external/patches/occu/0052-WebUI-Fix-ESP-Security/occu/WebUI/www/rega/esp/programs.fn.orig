    if( oTmp )
    {
      object oSCTmp = oTmp.CndAddSingle();
      if( oSCTmp )
      {
        if( oTmp.CndSingleCount() > 1 )
        {
          oSCTmp.OperatorType( oTmp.CndSingleCondition(0).OperatorType() );
        }
        else
        {
          if( oTmp.CndOperatorType() == 1 )
