//======================================================================
// Globale Variablen für diese Datei
SORT_DESC   = false;
IGNORE_CASE = true;
SORTED_COL  = -1;
SORT_ASC_SRC  = "/ise/img/arrow_up.gif";
SORT_DESC_SRC = "/ise/img/arrow_down.gif";
//======================================================================

//------------------------------------------------------------------------
//Klasse SelChannelBox für das PopUp Kanalauswahl:
/*
