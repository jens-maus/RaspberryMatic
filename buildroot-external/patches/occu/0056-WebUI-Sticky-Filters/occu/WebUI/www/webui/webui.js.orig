//Defines
BORDER_COLOR = WebUI.getColor("channelBorder");     //Border
HL_COLOR = WebUI.getColor("channelHighlight");  //Highlight
BG_COLOR = WebUI.getColor("channelBackground");//Background
HL_STROKE = 4;//Background
