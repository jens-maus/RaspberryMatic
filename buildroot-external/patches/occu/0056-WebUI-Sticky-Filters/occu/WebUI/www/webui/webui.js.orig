{
  var _separator = " ";
  var result = "";
  
  if (typeof(separator) != "undefined") { _separator = separator; }
  
  if (0 < this.length)
  {
    result = this[0][name];
    for (var i = 1, len = this.length; i < len; i++)
    {
      result += separator + this[i][name];
    }
  }
  
  return result;
}; 
 
