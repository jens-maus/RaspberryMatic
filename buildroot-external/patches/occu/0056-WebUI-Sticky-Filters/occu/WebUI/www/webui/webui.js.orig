if (PLATFORM == "Central") {

 DeviceListPage = Singleton.create(Page, {
  MAINMENU_ID: "MAINMENU_OPTIONS",
  TREE_COLLAPSED_FOOTER_HTML: "" +
    "<table border='0' cellspacing='8'>" +
      "<tr>" + 
