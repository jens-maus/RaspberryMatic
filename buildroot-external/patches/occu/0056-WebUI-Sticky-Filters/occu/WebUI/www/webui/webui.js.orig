  bindEvents: function () {

    var self = this;
    this.sliderInfoElm.on('change',function() {
      thisElm = jQuery(this);
      var value = parseInt(thisElm.val());

      if (isNaN(value)) {
        value = self.minVal;
        thisElm.val(value);
      } else if (value < self.minVal) {
        value = this.minVal;
        thisElm.val(value);
      } else if (value > self.maxVal) {
        value = self.maxVal;
        thisElm.val(value);
      }
      self.sliderVal = value;
      self.sliderElm.slider("value", value);
      self.setValue();
