  initialize: function(title, content, iseVal, callback, contentType)
  {
    var _this_ = this;

    this.iseVal = iseVal;

    this.m_contentType = contentType;
    this.m_callback = callback;
    this.m_layer = document.createElement("div");
    this.m_layer.className = "YesNoDialogLayer";
