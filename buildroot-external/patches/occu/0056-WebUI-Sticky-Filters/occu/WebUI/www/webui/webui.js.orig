        .setPosition(5,25)
        .setWidth(590)
        .setHeight(200)
        .setText('WriteLine("'+ translateKey("dialogScriptExecuterHelloWorld") +'");');
    
    this.m_output = new UI.Textarea()
        .setPosition(5,250)
        .setWidth(590)
        .setHeight(200); 
    
