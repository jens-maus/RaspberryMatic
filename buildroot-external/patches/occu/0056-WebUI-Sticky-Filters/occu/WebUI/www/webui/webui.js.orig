  {
    this.stepwidth = PROGRESSBAR_WIDTH;
    this.steppercent = 100;
  }
  else
  {
    this.stepwidth = Math.round(PROGRESSBAR_WIDTH / endcounter);
    this.steppercent = Math.round( 100 / endcounter );
  }

  this.AddDivWrapper("id_progressbar_wrapper");
