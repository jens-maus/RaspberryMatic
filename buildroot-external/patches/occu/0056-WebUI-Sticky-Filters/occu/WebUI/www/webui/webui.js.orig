/**
 * Geräteliste.
 **/
DeviceList = Singleton.create({

  /**
   * Konstruktor
   **/
  initialize: function()
  {
    this.m_addRoomHandler = this.m_onAddRoom.bind(this);
    this.m_removeRoomHandler = this.m_onRemoveRoom.bind(this);
    this.m_addChannelToRoomHandler      = this.m_onAddChannelToRoom.bind(this);
